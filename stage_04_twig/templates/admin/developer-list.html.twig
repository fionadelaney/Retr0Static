{% extends '_base.html.twig' %}

{# ------------------------------------------------- #}
{% block pageTitle %}Developers{% endblock %}

{# ------------------------------------------------- #}
{% block main %}

    <div class="container">
        <div class="form">
            <h1>Developers</h1>

            <table>
                <tr>
                    <th> Developer </th>
                    <th> &nbsp; </th>
                    <th> &nbsp; </th>
                    <th> &nbsp; </th>
                </tr>
                {% for item in developer_list %}
                    <tr>
                        <td>{{ item.name }}</td>
                        <td><a href="/index.php?admin/developer/view/{{ item.id }}" title="View" class="btn btn-view">View</a></td>
                        <td><a href="/index.php?admin/developer/edit/{{ item.id }}" title="Edit" class="btn btn-edit">Edit</a></td>
                        <td><a href="/index.php?admin/developer/delete/{{ item.id }}" title="Delete" class="btn btn-delete">Delete</a></td>
                    </tr>
                {% endfor %}
            </table>

        </div>
    </div>

{% endblock %}